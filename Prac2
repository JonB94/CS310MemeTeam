public class Parser {

	
	public boolean program(String sentence){
		
		
		String[] words = sentence.split(" ");
		
		int len = words.length;
		int count = 0;
		
		while(count != len){
			if(words[count] == "LET"){
				if(words[count+2] == "="){
					if(words[count+4] == ";"){
						
					}
					else break;
				}
				else break;
			}
			else break;
			
			
			
			
			count++;
		}
		
		return false;
	}
	
